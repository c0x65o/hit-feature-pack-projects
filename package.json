{
  "name": "@hit/feature-pack-projects",
  "version": "1.0.37",
  "description": "A HIT feature pack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "types": "./dist/schema/projects.d.ts",
      "import": "./dist/schema/projects.js",
      "require": "./dist/schema/projects.js",
      "default": "./dist/schema/projects.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@hit/feature-pack-forms": "*",
    "@hit/sdk": "*",
    "@hit/ui-kit": "*",
    "drizzle-orm": ">=0.30.0",
    "lucide-react": ">=0.263.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "zod": ">=3.22.0"
  },
  "devDependencies": {
    "@hit/feature-pack-forms": "file:../hit-feature-pack-forms",
    "@hit/sdk": "file:../../hit-sdks/hit-sdk-typescript",
    "@hit/ui-kit": "file:../hit-ui-kit",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "drizzle-orm": "^0.30.0",
    "lucide-react": "^0.460.0",
    "next": "^14.2.5",
    "typescript": "^5.9.3",
    "zod": "^3.24.1"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "projects"
  ],
  "license": "MIT"
}
